{
  "name": "mini-vue-devui",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "register:components": "vitepress-rc",
    "cli:dev": "esbuild --bundle ./devui-cli/index.js --format=cjs --platform=node --outdir=./lib --watch",
    "cli:build": "esbuild --bundle ./devui-cli/index.js --format=cjs --platform=node --outdir=./lib",
    "cli": "node ./lib/index.js create",
    "test": "jest"
  },
  "dependencies": {
    "vue": "^3.2.16"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vitejs/plugin-vue-jsx": "^1.2.0",
    "@vue/test-utils": "^1.2.2",
    "commander": "^8.3.0",
    "esbuild": "^0.13.10",
    "fs-extra": "^10.0.0",
    "inquirer": "^8.2.0",
    "jest": "^27.3.1",
    "kolorist": "^1.5.0",
    "sass": "^1.43.4",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vitepress": "^0.20.0",
    "vitepress-theme-demoblock": "^1.2.6",
    "vue-tsc": "^0.3.0"
  }
}
